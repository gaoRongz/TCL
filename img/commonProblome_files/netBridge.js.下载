define("KUYU.netBridge",[],function(){var e=KUYU.Init,n={go:function(e){window.location.href=e},back:function(){window.history.go(-1)},getEnvConfig:function(e){e()},getEnv:function(e){e()},showLoading:function(e){var n="";e&&(n=' style="background:rgba(242,242,242, '+e+');"'),$("body").append('<div id="loading"'+n+"></div>")},removeLoading:function(){$("#loading, #loading-body").fadeOut("slow",function(){this.remove()})},security:function(e){return e},fetch:function(e){e.security&&(e.data=n.security(e.data)),$.ajax(e)},getDeviceInfo:function(n){var i=localStorage.getItem("deviceToken");if(i)return void n({deviceFinger:"",deviceToken:i});var o=e.getEnv(),t=o.sever.toLocaleLowerCase(),c=e.getService(),r=c[t]+c.tk;if(!c.tk)return void n({deviceFinger:"",deviceToken:""});$.ajax({type:"post",url:r,success:function(e){var i=new Function("return"+e)();if(i.code!=CODEMAP.status.success||e.data)n({deviceFinger:"",deviceToken:""});else{var o=i.data.deviceToken||"";localStorage.removeItem("deviceToken"),localStorage.setItem("deviceToken",o),n({deviceFinger:"",deviceToken:o})}},error:function(e){n({deviceFinger:"",deviceToken:""})}})}};_APP.inject("KUYU.netBridge",n)});