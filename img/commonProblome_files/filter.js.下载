define("KUYU.Filter",[],function(){var e={price:function(e){return Number((e/100).toFixed(2))},toFiexd:function(e){return Number(e).toFixed(2)},date:function(e,t){e=new Date(e);var n={"M+":e.getMonth()+1,"D+":e.getDate(),"h+":e.getHours(),"m+":e.getMinutes(),"s+":e.getSeconds(),w:e.getDay(),"q+":Math.floor((e.getMonth()+3)/3),S:e.getMilliseconds()};/(Y+)/.test(t)&&(t=t.replace(RegExp.$1,(e.getFullYear()+"").substr(4-RegExp.$1.length)));for(var r in n)new RegExp("("+r+")").test(t)&&(t=t.replace(RegExp.$1,1==RegExp.$1.length?n[r]:("00"+n[r]).substr((""+n[r]).length)));return t},currency:function(e){},phonenum:function(e){if(void 0==e)return"";for(var t="",n=0;n<e.length;n++)t+=n>2&&n<7?"*":e[n];return t},formatCardNo:function(e){return e.substr(e.length-4)},formatBankImg:function(){return"../../images/icon/"+val.toLocaleLowerCase()+".png"}};_APP.inject("KUYU.Filter",e)});