define("KUYU.plugins.alert!CSS",[],function(){return Msg={Alert:function(t,n,i){this.GenerateHtml("alert",t,n),this.btnOk(i),this.btnNo()},Confirm:function(t,n,i){this.GenerateHtml("confirm",t,n),this.btnOk(i),this.btnNo()},GenerateHtml:function(t,n,i){var e="";e+='<div id="mb_box"></div><div id="mb_con"><span id="mb_tit">'+(n||"温馨提示")+"</span>",e+='<div id="mb_msg">'+i+'</div><div id="mb_btnbox">',"alert"==t&&(e+='<input id="mb_btn_ok" type="button" value="确定" />'),"confirm"==t&&(e+='<input id="mb_btn_no" type="button" value="取消" /><input id="mb_btn_ok" type="button" value="确定" />'),e+="</div></div>",$("body").append(e),this.GenerateCss()},GenerateCss:function(){var t=document.documentElement.clientWidth,n=document.documentElement.clientHeight,i=$("#mb_con").width(),e=$("#mb_con").height();$("#mb_con").css({top:(n-e)/2.6+"px",left:(t-i)/2+"px"})},btnOk:function(t){$("#mb_btn_ok").click(function(){if("function"==typeof t){if("false"==t())return;$("#mb_box,#mb_con").remove()}})},btnNo:function(){$("#mb_btn_no").click(function(){$("#mb_box,#mb_con").remove()})}},Msg});